{
  "name": "adminweb",
  "version": "0.9.3",
  "description": "Admin website - Frontend source codes",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist/* && parcel build --no-optimize --no-scope-hoist --no-content-hash src/index.html src/images/** --dist-dir ./dist --public-url .",
